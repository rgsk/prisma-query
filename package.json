{
  "name": "prisma-query",
  "version": "1.0.5",
  "description": "transforms rest query params to prisma model args",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "license": "MIT",
  "scripts": {
    "start": "NODE_PATH=dist-test node dist-test/server.js",
    "dev": "NODE_PATH=dist-test nodemon dist-test/server.js",
    "watch": "tsc -w",
    "build": "tsc",
    "prepublish": "parcel build",
    "kill:process": "kill -9 $(lsof -t -i:8000)",
    "init:prisma": "prisma generate",
    "studio": "prisma studio",
    "seeders": "node dist-test/seeds/rootSeed.js",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset",
    "migrate:force-reset": "prisma migrate reset --force",
    "test": "jest",
    "format": "prettier --write \"**/*.{ts,md}\""
  },
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.7.0",
    "@parcel/transformer-typescript-types": "2.7.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "jest": "^29.2.2",
    "jest-mock-extended": "^3.0.1",
    "nodemon": "^2.0.20",
    "parcel": "^2.7.0",
    "parcel-resolver-ts-base-url": "^1.3.0",
    "prettier": "^2.7.1",
    "prisma": "^4.5.0",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
